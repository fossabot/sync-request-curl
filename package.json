{
  "name": "sync-request-curl",
  "repository": {
    "type": "git",
    "url": "https://github.com/nktnet1/sync-request-curl"
  },
  "version": "1.1.2",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sync",
    "sync-request",
    "synchronous",
    "synchronous-request",
    "request",
    "fast",
    "quick",
    "request",
    "fast",
    "quick",
    "performant",
    "performant",
    "peformance",
    "http",
    "https",
    "curl",
    "libcurl",
    "node-libcurl"
  ],
  "author": "Khiet Tam Nguyen",
  "license": "MIT",
  "description": "Fast way to send synchronous web requests. API is a subset of the original sync-request, leverages node-libcurl",
  "dependencies": {
    "node-libcurl": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.5"
  }
}
